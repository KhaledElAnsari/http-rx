{
  "name": "http-rx",
  "version": "0.1.0",
  "description": "Reactive ajax http requests for the browser",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run clean && rollup -c && npm run build:min",
    "build:min": "rollup -c",
    "clean": "rimraf dist",
    "start": "live-server --no-browser",
    "watch": "npm run clean && rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1"
