  "license": "MIT",
  "dependencies": {
    "toy-rx": "^1.0.0",
    "zen-observable": "^0.5.1"
  },
  "devDependencies": {
    "buble": "^0.15.2",
    "rimraf": "^2.6.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-butternut": "^0.1.0",
    "rollup-plugin-cleanup": "^1.0.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^3.2.2"
  }
}
